{
  "name": "@fgiova/fastify-sqs-sns-publisher",
  "description": "A fastify plugin for publishing messages to SQS and SNS",
  "version": "0.0.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "clean": "rimraf dist",
    "build": "tsc",
    "test": "tap --before=test/scripts/executors/before.js --after=test/scripts/executors/teardown.js --node-arg=--require=ts-node/register",
    "test:debug": "tap -T -O --before=test/scripts/executors/before.js --after=test/scripts/executors/teardown.js --node-arg=--require=ts-node/register",
    "test:local": "TEST_LOCAL=true tap --before=test/scripts/executors/before.js --after=test/scripts/executors/teardown.js --node-arg=--require=ts-node/register",
    "test:local:debug": "TEST_LOCAL=true tap -T -O --before=test/scripts/executors/before.js --after=test/scripts/executors/teardown.js --node-arg=--require=ts-node/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fgiova/fastify-sqs-sns-publisher.git"
  },
  "author": "Francesco Giovannini <fgiova@fgiova.com>",
  "license": "MIT",
  "keywords": [
    "fastify",
    "sqs",
    "sns",
    "aws",
    "amazon"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "tap": {
    "ts": true,
    "lines": 100,
    "branches": 100,
    "statements": 100,
    "functions": 90
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.15.0",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.1.0",
    "eslint-plugin-import": "^2.28.1",
    "fastify": "^4.21.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "semantic-release": "^21.0.9",
    "tap": "^16.3.4",
    "testcontainers": "^10.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.395.0",
    "@aws-sdk/client-sqs": "^3.395.0",
    "fastify-plugin": "^4.5.1"
  }
}
